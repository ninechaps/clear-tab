{
  "manifest_version": 3,
  "name": "Clear Chrome Tab",
  "version": "1.0.0",
  "description": "A clean and luminous new tab experience.",
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "https://zenquotes.io/*"
  ],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
